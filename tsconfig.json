{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "baseUrl": "types",
        "typeRoots": [
            "types"
        ],
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    }
}